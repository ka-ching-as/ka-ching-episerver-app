<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <section name="kaching" xdt:Transform="Remove" xdt:Locator="Match(name)" />
  </configSections>

  <episerver.shell>
    <protectedModules>
      <add name="KachingPlugIn" xdt:Transform="Remove" xdt:Locator="Match(name)"/>
    </protectedModules>
  </episerver.shell>

  <kaching xdt:Transform="Remove"/>
</configuration>
