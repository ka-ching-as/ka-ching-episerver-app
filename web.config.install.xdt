<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <section name="kaching" type="KachingPlugIn.Configuration.KachingConfiguration, KachingPlugIn" />
  </configSections>

  <episerver.shell xdt:Transform="InsertIfMissing">
    <protectedModules rootPath="~/EPiServer/" xdt:Transform="InsertIfMissing">
      <add name="KachingPlugIn" xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)"/>
    </protectedModules>
  </episerver.shell>

  <kaching productsImportUrl=""
           foldersImportUrl=""
           tagsImportUrl=""
           exportSingleVariantAsProduct="false">
    <systemMappings barcodeMetaField=""
                    descriptionMetaField=""/>
    <attributeMappings>
      <!--<add metaField="Brand" attributeId="brand"/>-->
    </attributeMappings>
  </kaching>
</configuration>
